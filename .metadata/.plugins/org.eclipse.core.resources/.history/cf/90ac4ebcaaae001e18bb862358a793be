package Chapter9;

public class Studentc {
	String name;
	
	@Override
	public String toString() {
		return "Studentc [name=" + name + "]";
	}

	public static void main(String[] args)  {	
		var student = new Studentc();
		System.out.println(student);
		student.name = "Justin";
		System.out.println(student);
	}
}
