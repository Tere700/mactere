package iii.brad.tw;

public class RuntimeExcept {

	public static void main(String[] args) {
		int a = 10, b = 10;
		int[] c = {1, 2, 3, 4};

		try {
			int d = a / b;
		

			System.out.println(d);
			System.out.println(c[33]);
		} catch (RuntimeException e) {
			System.out.println("XXXXX");
		}

	}

}
