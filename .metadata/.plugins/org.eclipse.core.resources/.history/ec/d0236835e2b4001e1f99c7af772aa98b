package iii.brad.tw;

public class RuntimeExcept {

	public static void main(String[] args) {
		int a = 10, b = 0;
		int[] c = {1, 2, 3, 4};

		try {
			int d = a / b;
		
			System.out.println(c[3]);
			System.out.println(d);
		} catch (RuntimeException e) {
			System.out.println("XXXXX");
		}

	}

}
